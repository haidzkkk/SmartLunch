<script>

	// Khởi tạo biến số lượng user
	var numberUser = 1;
	var numberUser2 = 1;

	// Tạo hiệu ứng animation
	function animateNumberUser() {
		// Thay đổi giá trị của biến số lượng user
		numberUser++;
		numberUser2++;

		// Cập nhật giá trị của số lượng user trên màn hình
		document.getElementById("numberUser").innerHTML = numberUser;
		document.getElementById("numberUser2").innerHTML = numberUser2;

		// Nếu giá trị của số lượng user chưa đạt đến 100 thì tiếp tục chạy hiệu ứng
		if (numberUser <= 100) {
			setTimeout(animateNumberUser, 5);
		}
	}
	if (numberUser2 <= 100) {
		setTimeout(animateNumberUser, 5);
	}

	// Khởi chạy hiệu ứng animation
	animateNumberUser();
</script>